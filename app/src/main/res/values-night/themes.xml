<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="WhiteTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/whiteColorPrimary</item>
        <item name="colorPrimaryDark">@color/whiteColorPrimaryDark</item>
        <item name="colorSecondary">@color/whiteColorSecondary</item>
        <item name="android:backgroundTint">@color/whiteColorPrimary</item>
